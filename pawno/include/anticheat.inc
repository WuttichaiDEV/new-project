stock IsAirTransportAC(veh_id)
{
	new model = GetVehicleModel(veh_id);
	if(!model) return 0;
	return VehicleAir[model - 400];
}

stock IsWaterTransportAC(_id_vehicle)
{
    new model = GetVehicleModel(_id_vehicle);
	if(!model) return 0;
	return VehicleWater[model - 400];
}



stock GetVehicleDriverIDAC(_id_vehicle)
{
	if(IsPlayerInVehicle(vehicleOccupied[_id_vehicle][0], _id_vehicle) && 
	   GetPlayerState(vehicleOccupied[_id_vehicle][0]) == PLAYER_STATE_DRIVER) 
	   		return vehicleOccupied[_id_vehicle][0];
	return 0xFFFF;
}

stock IsVehicleOccupiedAC(_id_vehicle)
{
	for(new iter = 8; iter >= 0; iter--) if(IsPlayerInVehicle(vehicleOccupied[_id_vehicle][iter], _id_vehicle)) return 1;
	return 0;
}

stock GetTickDiffAC(newtick, oldtick)
{
	if(oldtick > newtick || (oldtick >= 0 && newtick < 0)) return ((cellmax - oldtick + 1) - (cellmin - newtick));
	return (newtick - oldtick);
}

stock GetPlayerSpeedAC(playerid)
{
    new Float:vel_x, Float:vel_y, Float:vel_z, _id_vehicle = GetPlayerVehicleID(playerid);
    if(!_id_vehicle) GetPlayerVelocity(playerid, vel_x, vel_y, vel_z);
	else GetVehicleVelocity(_id_vehicle, vel_x, vel_y, vel_z);
	return floatround(VectorSize(vel_x, vel_y, 0.0) * 136.666667);
}


stock IsPlayerInWaterAC(playerid)
{
	new Float:pos_z;
    GetPlayerPos(playerid, pos_z, pos_z, pos_z);
	if(pos_z < 0.0) return 1;
	for(new iter = (sizeof(WaterPos) - 1); iter >= 0; iter--) if(GetPlayerDistanceFromPoint(playerid, WaterPos[iter][0], WaterPos[iter][1], WaterPos[iter][2]) <= WaterPos[iter][3]) return 1;
	return 0;
}

stock IsVehicleInWaterAC(_id_vehicle)
{
	new Float:pos_z;
    GetVehiclePos(_id_vehicle, pos_z, pos_z, pos_z);
	if(pos_z < 0.0) return 1;
	for(new iter = (sizeof(WaterPos) - 1); iter >= 0; iter--) if(GetVehicleDistanceFromPoint(_id_vehicle, WaterPos[iter][0], WaterPos[iter][1], WaterPos[iter][2]) <= WaterPos[iter][3]) return 1;
	return 0;
}

stock IsComponentidCompatibleAC(modelid, componentid)
{
	if(1000 <= componentid <= 1191)
	{
		componentid -= 1000;
		if(VehicleMods[modelid - 400][componentid >>> 5] & (1 << (componentid & 0b00011111))) return 1;
	}
	else if((componentid == 1192 || componentid == 1193) && modelid == 576) return 1;
	return 0;
}

stock IsPlayerFacingCoordsAC(playerid, Float:dest_x, Float:dest_y, Float:offset = 5.0)
{
    new Float:pos_x, Float:pos_y, Float:pos_z, Float:pos_a, Float:tmp1, Float:tmp2, Float:tmp3 = 5.0;
    GetPlayerPos(playerid, pos_x, pos_y, pos_z);
    GetPlayerFacingAngle(playerid, pos_a);
    tmp2 = (180.0 - atan2((pos_x - dest_x), (pos_y - dest_y)));
    tmp2 += tmp3;
    tmp3 *= -1;
    tmp1 = (tmp2 + tmp3);
    if((tmp1 + offset) > 360.0) tmp1 = (360.0 - offset);
    if((tmp1 - offset) < 0.0) tmp1 = (0.0 + offset);
    if((tmp1 - offset) < pos_a < (tmp1 + offset)) return 1;
    return 0;
}

stock SleepTime(playerid)
{
	new _count = (3 + floatround((GetPlayerPing(playerid) > AC_MAX_PING) ? (AC_MAX_PING / 100) : (GetPlayerPing(playerid) / 100)));
	return (_count < 3) ? (3) : (_count > 10) ? (10) : (_count);
}
stock PublicFlood(playerid, iter)
{
    if(GetTickDiffAC(GetTickCount(), playerLastPublicTick[playerid][iter]) < PublicMaxTick[iter]) { if(++playerWarningPublic[playerid][iter] > 10)
	{
		CheatDetected(playerid, AC_PUBFLOOD);
	}}
	else playerWarningPublic[playerid][iter] = 0;
	if(GetTickDiffAC(GetTickCount(), playerLastPublicTick[playerid][27]) < PublicMaxTick[27]) { if(++playerWarningPublic[playerid][27] > 10) CheatDetected(playerid, AC_CROSFLOOD); }
	else playerWarningPublic[playerid][27] = 0;
	playerLastPublicTick[playerid][iter] =
	playerLastPublicTick[playerid][27] = GetTickCount();
}
stock CheatDetected(playerid, code) 
{ 
	if(!playerKickTimer[playerid] && IsPlayerConnected(playerid))
	{
		if ( p_info [ playerid ] [ admin ] > 0 ) return 1 ;
		new _ret = 1;

		if(_ret)
		{
			new  _t_string [ 128 ] ;
			format ( _t_string, sizeof _t_string, "{FFFFFF}Вы были кикнуты системой защиты!\n\nКод: {"#cRD"}#%d{FFFFFF}\nВведите {"#cRD"}/q", code ) ;
			show_dialog ( playerid, d_none, DIALOG_STYLE_MSGBOX,"{"#cRD"}Соединение разорвано", _t_string, "Закрыть", "") ;
			kick_player ( playerid ) ;
			return 1 ;
		}
	}
	return 1 ;
}


