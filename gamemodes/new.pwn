CMD:su(playerid, params[])
{
    if(!cop_player(playerid) && p_info[playerid][member] != 7) return 1;
    if(!is_fraction_duty{playerid}) return SendClientMessage(playerid, col_white, !""scm_error"Anda belum memulai hari kerja Anda.");
    if(p_info[playerid][rank] < 2) return SendClientMessage(playerid, col_white, !""scm_error"Perintah ini tersedia dari peringkat 2.");
    extract params -> new player:targetid, su_wanted, string:su_reason[64]; else return SendClientMessage(playerid, col_gray, !"Gunakan: "c_white"/su [id pemain] [wanted] [alasan]");
    if(!IsPlayerConnected(targetid) || targetid == playerid) return SendClientMessage(playerid, col_white, !""scm_error"Pemain tidak ditemukan.");
    if(su_wanted < 1 || su_wanted > 6) return SendClientMessage(playerid, col_white, !""scm_error"Level yang wanted harus antara 1 dan 6.");
    if(p_info[playerid][jailed] > 0) return SendClientMessage(playerid, col_white, !""scm_error"Tidak bisa memasukkan tahanan ke dalam daftar buronan.");
    if(pl_afk_time[targetid] > 30) return SendClientMessage(playerid, col_white, !""scm_error"Pemain ini di AFK. Dia tidak bisa diberikan wanted level.");
    if(p_info[targetid][wanted] + su_wanted > 6) return SendClientMessage(playerid, col_white, !""scm_error"Anda tidak dapat memberi pemain lebih dari 6 bintang.");
	if(cop_player(targetid)) return SendClientMessage(playerid, col_white, !""scm_error"Anda tidak dapat mengeluarkan pengacara yang wanted.");

    p_info[targetid][wanted] = su_wanted;
    SetPlayerWantedLevel(targetid, p_info[targetid][wanted]);
    update_int_sql(targetid, "u_wanted", p_info[targetid][wanted]);

    new text_string[144];
    format(text_string, sizeof(text_string), "%s %s menempatkan Anda pada daftar orang yang dicari. Alasan:%s. Level wanted saat ini: %d", f_rank[p_info[playerid][member] - 1][p_info[playerid][rank] - 1], p_info[playerid][name], su_reason, p_info[targetid][wanted]);
    SendClientMessage(targetid, col_orange_red, text_string);

    format(text_string, sizeof(text_string), "Anda telah menempatkan %s pada daftar yang di wanted. Alasan:%s. Level wanted saat ini %d", p_info[targetid][name], su_reason, p_info[targetid][wanted]);
    SendClientMessage(playerid, 0x0099FFFF, text_string);

    format(text_string, sizeof(text_string), "%s %s[%d] Wanted %s [%d] [%d/6], alasan: %s", f_rank[p_info[playerid][member] - 1][p_info[playerid][rank] - 1], p_info[playerid][name], playerid, p_info[targetid][name], targetid, p_info[targetid][wanted], su_reason);
    foreach(new i: logged_players) if(cop_player(i) || fbi_player(i)) SendClientMessage(i, 0xFFD700FF, text_string);
    return 1;
}

