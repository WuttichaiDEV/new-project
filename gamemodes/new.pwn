CMD:asellbiz(playerid, params[])
{
	if(p_info[playerid][admin] < 7) return true;

	new b, isMsg;
	if(sscanf(params, "dD(1)", b, isMsg)) return SendClientMessage(playerid, col_white, !"Gunakan: /asellbiz [id bisnis]");

	if(!(0 < b < b_count))
	{
		printf("%d", b);
		return SendClientMessage(playerid, col_gray, !"Bisnis tidak dikenal");
	}

	new b_owner = GetPlayerID(b_info[b-1][b_owner_name]);

	if(b_info[b-1][b_owner_inc] == -1)
		return SendClientMessage(playerid, col_gray, !"Bisnis milik negara");

	if(IsPlayerConnected(b_owner))
	{
		update_int_sql(b_owner, "u_business", -1);
		p_info[b_owner][business] = -1;
		SendClientMessage(b_owner, col_light_red, "Administrator menjual bisnis Anda");
	}
	else
	{
		new query_string[87];
		format(query_string,sizeof(query_string),"UPDATE `users` SET `u_business` = '-1' WHERE `u_id` = '%d' LIMIT 1", b_info[b-1][b_owner_inc]);
		mysql_tquery(mysql, query_string, "", "");
	}

	b_info[b-1][b_owner_inc] = -1;
	strfmt(b_info[b-1][b_owner_name], "The State");

	if(isMsg == 1) SendClientMessage(playerid, col_white, "Bisnis berhasil dijual");

	PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
	new query_string[200 + MAX_PLAYER_NAME + 1];
	format(query_string, 136,"UPDATE `businesses` SET `b_owner_inc` = '-1', `b_owner_name` = 'The State' WHERE `b_id` = '%d' LIMIT 1", b_info[b-1][b_id]);
	mysql_tquery(mysql, query_string);

