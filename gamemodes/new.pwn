callback: callback_debtor_bankmoney ( playerid, pl_name [ ] )
{
	new rows, fields;
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	if ( rows )
	{
		new pl_id, pl_bank, pl_tax ;

		cache_get_value_name_int ( 0, "u_id", pl_id ) ;
		cache_get_value_name_int ( 0, "u_bank", pl_bank ) ;
		cache_get_value_name_int ( 0, "u_tax", pl_tax ) ;

		if ( pl_bank < 1 )
		{
			SendClientMessage ( playerid, col_gray,""c_orange_red"* "c_grey"Pemain tidak memiliki dana di rekening bank utama." ) ;

			new pvar_string [ 38 ] ;


			format ( pvar_string, sizeof ( pvar_string ), ""c_server"%s", pl_name ) ;
			show_dialog ( playerid, d_debtors_pl_menu, DIALOG_STYLE_LIST, pvar_string, "Informasi tentang properti\nJual properti\nUntuk menarik uang dari rekening bank\nKirim pemberitahuan", "Pilih", "Batal" ) ;
			return 1 ;
		}

		new player_id, _text_string [ 164 ] ;
		sscanf ( pl_name, "u", player_id ) ;
		if ( IsPlayerConnected ( player_id ) )
		{
			if ( p_info [ player_id ] [ bank ] > pl_tax )
			{
				p_info [ player_id ] [ bank ] -= pl_tax ;
				format(_text_string, 128,"UPDATE `users` SET `u_tax` = '0',`u_bank`='%d' WHERE `u_id` = '%d' LIMIT 1",
				p_info [ player_id ] [ bank ], pl_id ) ;
				mysql_tquery ( mysql, _text_string ) ;

				p_info [ player_id ] [ tax ] = 0 ;

				f_info [ 10 ] [ f_money ] += pl_tax ;
				format ( _text_string, sizeof ( _text_string ), "UPDATE `fractions` SET `f_money` = '%d' WHERE `f_id` = '11' LIMIT 1",
				f_info [ 10 ] [ f_money ] ) ;
				mysql_tquery ( mysql, _text_string, "", "" ) ;
			}
			else
			{
				format(_text_string, 128,"UPDATE `users` SET `u_tax` = u_tax-'%d',`u_bank`='0' WHERE `u_id` = '%d' LIMIT 1",
				p_info [ player_id ] [ bank ], pl_id ) ;
				mysql_tquery ( mysql, _text_string ) ;

				p_info [ player_id ] [ tax ] -= p_info [ player_id ] [ bank ] ;

				f_info [ 10 ] [ f_money ] += p_info [ player_id ] [ bank ] ;
				format ( _text_string, sizeof ( _text_string ), "UPDATE `fractions` SET `f_money` = '%d' WHERE `f_id` = '11' LIMIT 1",
				f_info [ 10 ] [ f_money ] ) ;
				mysql_tquery ( mysql, _text_string, "", "" ) ;

				p_info [ player_id ] [ bank ] = 0 ;
			}
			SendClientMessage ( player_id, col_gray, "* Karyawan kantor walikota telah mendebit dana dari rekening Anda sebagai pajak." ) ;
			SendClientMessage ( player_id, col_gray, "* Lain kali, berhati-hatilah dengan pajak Anda." ) ;

		}
		else
		{
			format ( _text_string, sizeof _text_string, "INSERT INTO `debtor_message`(`dm_text`, `dm_dest`) VALUES ('"c_white"Sebagian dana didebit dari rekening bank Anda untuk melunasi hutang pajak.','%d')",
			GetPVarInt ( playerid, "debt_uid" ) ) ;
			mysql_tquery ( mysql, _text_string ) ;
			if ( pl_bank > pl_tax )
			{
				pl_bank -= pl_tax ;
				format(_text_string, 128,"UPDATE `users` SET `u_tax` = '0',`u_bank`='%d' WHERE `u_id` = '%d' LIMIT 1",
				pl_bank, pl_id ) ;
				mysql_tquery ( mysql, _text_string ) ;

				f_info [ 10 ] [ f_money ] += pl_tax ;
				format ( _text_string, sizeof ( _text_string ), "UPDATE `fractions` SET `f_money` = '%d' WHERE `f_id` = '11' LIMIT 1",
				f_info [ 10 ] [ f_money ] ) ;
				mysql_tquery ( mysql, _text_string, "", "" ) ;
			}
			else
			{
				format(_text_string, 128,"UPDATE `users` SET `u_tax` = u_tax-'%d',`u_bank`='0' WHERE `u_id` = '%d' LIMIT 1",
				pl_bank, pl_id ) ;

				mysql_tquery ( mysql, _text_string ) ;

				f_info [ 10 ] [ f_money ] += pl_bank ;
				format ( _text_string, sizeof ( _text_string ), "UPDATE `fractions` SET `f_money` = '%d' WHERE `f_id` = '11' LIMIT 1",
				f_info [ 10 ] [ f_money ] ) ;
				mysql_tquery ( mysql, _text_string, "", "" ) ;
			}
		}
		SendClientMessage ( playerid, col_white, ""c_green"* "c_white"Dana debitur telah berhasil didebet dari rekeningnya." ) ;
		DeletePVar ( playerid, "debt_house" ) ;
		DeletePVar ( playerid, "debt_business" ) ;
		DeletePVar ( playerid, "debt_tax" ) ;
		DeletePVar ( playerid, "debt_uid" ) ;
		DeletePVar( playerid, "ofm_listitem" ) ;
		mysql_tquery ( mysql, "SELECT `u_name`,`u_tax` FROM `users` WHERE `u_tax` > 10000 ORDER BY `users`.`u_tax` DESC", "callback_debtors", "i", playerid ) ;
	}
	return 1 ;
}



