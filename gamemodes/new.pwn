cmd:fambizwar(playerid)
{
    if(!p_info[playerid][family]) return SendClientMessage(playerid, col_white, !""scm_error"Anda bukan anggota keluarga.");
    if(!family_info[p_info[playerid][family] - 1][fam_type]) return SendClientMessage(playerid, col_white, !""scm_error"Tipe keluarga Anda tidak cocok untuk perintah ini.");

	if(p_info[playerid][family_rang] < family_info[p_info[playerid][family] - 1][fam_settings][0])
	{
		new text_string[100];
		format(text_string, sizeof(text_string), ""scm_error"Perintah tersedia dari status %s (%d).", family_rank[p_info[playerid][family] - 1][family_info[p_info[playerid][family] - 1][fam_settings][0] - 1], family_info[p_info[playerid][family] - 1][fam_settings][0]);
		return SendClientMessage(playerid, col_white, text_string);
	}

    if ( fam_biz != -1 ) return SendClientMessage ( playerid, col_white, !""scm_error"Perang amunisi sudah berlangsung." ) ;

	if ( family_cd > 0 )
	{
		new text_str [ 78 ] ;
		format ( text_str, sizeof ( text_str ), ""scm_error"Tangkap amunisi akan tersedia melalui %s", convert_time ( family_cd ) ) ;
		return SendClientMessage ( playerid, col_white, text_str ) ;
	}

	for ( new b = 0; b < b_count ; b ++ )
	{
		if ( IsPlayerInRangeOfPoint ( playerid, 3.0, b_info [ b ] [ b_position ] [ 0 ] , b_info [ b ] [ b_position ] [ 1 ], b_info [ b ] [ b_position ] [ 2 ] ) )
		{
			if(b_info [ b ] [ b_type ] == bizz_type_ammo )
			{
				if ( b_info [ b ] [ b_family ] == p_info [ playerid ] [ family ] )
				{
					SendClientMessage ( playerid, col_white, !""scm_error"Anda tidak dapat mulai mengambil alih bisnis Anda." ) ;
					return 1 ;
				}
				if ( b_info [ b ] [ b_family ] < 1 )
				{
					b_info [ b ] [ b_family ] = p_info [ playerid ] [ family ] ;

					new query_string [ 228 ];

					format ( query_string, sizeof ( query_string ), ""c_server"%s (No%d)\n"c_grey"Pemilik: "c_white"%s\n"c_grey"Harga: "c_white"%d$\n"c_grey"Keluarga: "c_white"%s\n"c_grey"kondisi: "c_white"%s",
					b_types [ b_info [ b ] [ b_type ] ], b_info [ b ] [ b_id ], b_info [ b ] [ b_owner_name ], b_info [ b ] [ b_price ], family_info[b_info[b][b_family] - 1][fam_name], ( ! b_info [ b ] [ b_close ]) ? ("{1a9616}Buka") : ("{e11010}Tutup") ) ;
					UpdateDynamic3DTextLabelText ( b_info [ b ] [ b_label ], -1, query_string ) ;

					format ( query_string, 118,"UPDATE `businesses` SET `b_family` = '%d' WHERE `b_id` = '%d' LIMIT 1", b_info [ b ] [ b_family ], b_info [ b ] [ b_id ] ) ;
					mysql_tquery ( mysql, query_string ) ;

					SendClientMessage ( playerid, col_white, !""scm_sucess"Amunisi dikendalikan." ) ;
					return 1 ;
				}

				SendClientMessage ( playerid, col_white, !""scm_info"Pilih lokasi di peta untuk menangkap amunisi." ) ;
				SendClientMessage ( playerid, col_white, !""scm_info"Ini harus menjadi area di pinggiran kota Las Venturas." ) ;
				SetPVarInt ( playerid, "fam_war_map", 1 ) ;
				return 1 ;
			}
		}
	}
	SendClientMessage ( playerid, col_white, !""scm_error"Anda keluar dari bisnis." ) ;

	return 1;
}

