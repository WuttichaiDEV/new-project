callback: dellaccount_check_callback ( playerid, pl_name [ ] )
{
	new rows, fields;
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	if ( ! rows )
	{
		new scm_string [ 128 ] ;
		format( scm_string, sizeof ( scm_string ), ""c_orange_red"* "c_orange_red"Akun %s tidak ditemukan dalam database.", pl_name ) ;
		SendClientMessage ( playerid, col_gray, scm_string ) ;
		return 1 ;
	}
	else
	{
		static query6[] = "DELETE IGNORE FROM "TABLE_PHOTOGRHAPHER_COOLDOWN" WHERE `user_name` = '%e' LIMIT 1";
		new fmt_query6[sizeof(query6) + MAX_PLAYER_NAME + 1 - 2];
		mysql_format(mysql, fmt_query6, sizeof(fmt_query6), query6, pl_name);
		mysql_tquery(mysql, fmt_query6);

		new scm_string [ 108 ] ;
		format ( scm_string, sizeof ( scm_string  ), "DELETE FROM `users` WHERE `u_name` = '%s' LIMIT 1", pl_name ) ;
		mysql_tquery ( mysql, scm_string ) ;

		format ( scm_string, sizeof ( scm_string  ),""c_grey" [A] %s[%d] akun yang dihapus %s.", p_info [ playerid ] [ name ], playerid, pl_name ) ;
		foreach(new i: admin_players)SendClientMessage ( i, col_gray, scm_string ) ;
		return 1 ;
	}
}

