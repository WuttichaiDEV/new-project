		else
		{
			format ( scm_string, sizeof ( scm_string ), "UDPATE `users` SET `u_f_warning` = '0', `u_member` = '0', `u_rank` = '0' WHERE `u_name` = '%s' LIMIT 1", pl_name ) ;
