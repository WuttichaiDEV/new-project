	veh_info [ vehicleid - 1 ] [ v_driver ] = INVALID_PLAYER_ID ;

	veh_info [ vehicleid - 1 ] [ v_cargo ] = 0 ;

	new engine, lights, alarm, doors, bonnet, boot, objective ;

