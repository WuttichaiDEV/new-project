cmd:alogin(playerid)
{
	if(p_info[playerid][admin])
	{
		new text_string[95];
		format(text_string, sizeof(text_string), ""c_grey" [A] %s[%d] keluar dari panel admin", p_info[playerid][name], playerid);
		foreach(new i: admin_players) SendClientMessage(i, col_gray, text_string);

		Iter_Remove(admin_players, playerid);
		p_info[playerid][admin] = 0;
		for(new i; i < 10; i++) TextDrawHideForPlayer(playerid, ch_td[0][i]), TextDrawHideForPlayer(playerid, ch_td[1][i]);
		return 1;
	}
	new tquery_string[85];
	mysql_format(mysql, tquery_string, sizeof(tquery_string), "SELECT * FROM `users_admins` WHERE `u_a_name`='%s' LIMIT 1", p_info[playerid][name]);
	mysql_tquery(mysql, tquery_string, "alogin_step_one", "i", playerid);
	return 1;
}

callback: alogin_step_one(playerid)
{
	new rows, fields ;
	cache_get_row_count(rows);
	cache_get_field_count(fields);
	
    if(!rows) return 1;

    new admin_pass[32];
