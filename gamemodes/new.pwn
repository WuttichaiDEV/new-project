    new ips[20];
	if(sscanf(params,"s[20]",ips)) return SendClientMessage(playerid, -1, "Gunakan: /boton [ip]");
	if(strlen(ips) < 1 || strlen(ips) > 20) return SendClientMessage(playerid, -1, "Anda memasukkan jumlah karakter yang salah");

	new bot_iter = - 1 ;
	for ( new j = 0 ; j < 5 ; j ++  )
	{
		if ( strlen ( ip_white_list [ j ] ) > 3 ) continue ;
		bot_iter = j ;
	}
	if ( bot_iter == - 1 ) return SendClientMessage(playerid, -1, "Tidak ada ruang kosong di daftar putih alamat IP.");

	format ( ip_white_list [ bot_iter ], 16, "%s", ips ) ;
	SendClientMessage(playerid, -1, ip_white_list [ bot_iter ]);
	SendClientMessage(playerid, -1, "IP ditambahkan ke daftar putih.");
	return 1 ;
}
