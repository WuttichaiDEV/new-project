cmd:startmp(playerid,params[])
{
	if ( p_info [ playerid ] [ admin ] < 3 ) return 1 ;
    new
           Float:mp_pos_x,
           Float:mp_pos_y,
           Float:mp_pos_z;

	new quantity_player, name_mp[32];
	if ( sscanf ( params, "ds[32]", quantity_player, name_mp ) ) return SendClientMessage ( playerid, col_gray, ""c_orange_red"* "c_grey"Gunakan: /startmp [Jumlah pemain] [Nama acara]" ) ;

    if(GetSVarInt("AidPlayers"))
       return SendClientMessage(playerid, col_gray,""c_orange_red"* "c_grey"Pemain berkumpul untuk acara tersebut.");
    if(quantity_player > 100)
       return SendClientMessage(playerid, col_gray,""c_orange_red"* "c_grey"Jumlah maksimal pemain per acara: 100.");
    if(!(1 <= strlen(params) <= 32))
       return SendClientMessage(playerid, col_gray,""c_orange_red"* "c_grey"Nama acara terlalu panjang.");
    SetSVarInt("AidPlayers",quantity_player);
    GetPlayerPos(playerid, mp_pos_x, mp_pos_y, mp_pos_z);
    SetSVarFloat("mp_pos_x",mp_pos_x);
    SetSVarFloat("mp_pos_y",mp_pos_y);
    SetSVarFloat("mp_pos_z",mp_pos_z);
	SetSVarInt("mp_int",GetPlayerInterior ( playerid ) );
	SetSVarInt("mp_virt",GetPlayerVirtualWorld ( playerid ) );

    new string[ 128 ] ;
    format(string,sizeof(string),""c_yellow"* S.INFO: Administrator %s telah memulai perkumpulan pemain untuk acara '%s'. Maksimal:%d orang", p_info [ playerid ] [ name ], name_mp,quantity_player);
    SendClientMessageToAll(col_gray,string);

    return SendClientMessageToAll(col_gray, ""c_yellow"* S.INFO: Gunakan /gomp untuk teleportasi ke acara tersebut");
}

cmd:stopmp(playerid)
{
	if ( p_info [ playerid ] [ admin ] < 3 ) return 1 ;
    if(!GetSVarInt("AidPlayers")) return SendClientMessage(playerid, col_white, !""scm_error"Koleksi pemain tidak berjalan");
    
    DeleteSVar("AidPlayers");
    DeleteSVar("mp_pos_x"); DeleteSVar("mp_pos_y"); DeleteSVar("mp_pos_z");
	DeleteSVar("mp_int");  DeleteSVar("mp_virt");
	
    return SendClientMessageToAll(col_white, !""c_yellow"* S.INFO: Pengumpulan pemain untuk acara tersebut dihentikan oleh administrator");
}


cmd:gomp(playerid)
{
	if(!GetSVarInt("AidPlayers")) return SendClientMessage(playerid, col_white, !""scm_error"The collection of players is not running");
       
    if ( p_info [ playerid ] [ jailed ] > 0 ) return SendClientMessage ( playerid, col_white, !""scm_error"Anda tidak dapat berpartisipasi dalam acara tersebut saat berada di penjara.") ;
	if ( GetSVarInt("AidPlayers") < 2 )
    {
        SendClientMessage ( playerid, col_white, !""c_yellow"* S.INFO: Pengumpulan pemain untuk acara sudah berakhir." ) ;
        DeleteSVar("mp_pos_x");  DeleteSVar("mp_pos_y"); DeleteSVar("mp_pos_z");
		DeleteSVar("AidPlayers");
		DeleteSVar("mp_int");  DeleteSVar("mp_virt");
		return 1 ;
    }
    SetSVarInt("AidPlayers",GetSVarInt("AidPlayers")-1);

    set_pos ( playerid, GetSVarFloat("mp_pos_x") + random(5), GetSVarFloat("mp_pos_y") + random(5), GetSVarFloat("mp_pos_z"), 0.0, GetSVarInt("mp_int"), GetSVarInt("mp_virt") ) ;

    return SendClientMessage(playerid, col_white, !""c_yellow"* S.INFO: Anda telah dipindahkan ke acara, menunggu perintah administrasi.");
}

cmd:dmgstat ( playerid, params [ ] )
{
