stock AddItem(playerid,item_id,num_cell,drop)
{
	switch(drop)
	{
		case 0:
		{
			new full, string[250];
			for(new i = 1; i < 29; i++)
			{
			    if(p_info[playerid][Inventory][i]) continue;
			    p_info[playerid][Inventory][i] = item_id;
		        p_info[playerid][Inventory_cell][i] = num_cell;
		        full = 1;
       			f(string,"Item baru telah ditambahkan ke inventaris Anda: '%s' dalam jumlah: '%d'",Items_Name(GetPVarInt(playerid,"item_id")),num_cell);
				SendClientMessage(GetPVarInt(playerid,"player_id"),0xFFC618FF,string);
				break;
			}
			if(!full) return SendErrorMessage(playerid,"Tidak ada ruang di inventaris!");
			SavePlayerInventory(playerid);
		}
	}
	return 1;
}
stock SavePlayerInventory(playerid)
{
   	new mysql_text[256],string[158];
	for(new i; i < 30; i++)
	{
	    if(!p_info[playerid][Inventory][i]) p_info[playerid][Inventory_cell][i] = 0;
		f(string,"%s%d,",string,p_info[playerid][Inventory][i]);
	}
	format(mysql_text, sizeof(mysql_text), "UPDATE `users` SET `pInventory` = '%s' WHERE `u_id` = '%d'",string,p_info[playerid][id]);
	mysql_tquery(mysql, mysql_text, "", "");

    string = "";
	for(new i; i < 30; i++) f(string,"%s%d,",string,p_info[playerid][Inventory_cell][i]);
	format(mysql_text, sizeof(mysql_text), "UPDATE `users` SET `pInventory_cell` = '%s' WHERE `u_id` = '%d'",string,p_info[playerid][id]);
	mysql_tquery(mysql, mysql_text, "", "");
	return 1;
}
stock Update_Inventory(playerid, id_cell,param)
{
	if(param == 0) PlayerTextDrawBackgroundColor(playerid, InventorySlots[id_cell], COLOR_1);
	if(param == 1) PlayerTextDrawBackgroundColor(playerid, InventorySlots[id_cell], COLOR_3);
	PlayerTextDrawHide(playerid, InventorySlots[id_cell]);
	PlayerTextDrawShow(playerid, InventorySlots[id_cell]);
	return true;
}
stock Check_Inventory(params, param)
{
    switch(param)
    {
        case 0: // Проверка на скины
		{
			switch(params)
			{
			    case 1..17,19..44,46..73,75..137,140..251,253..311: return true;
			}
		}
