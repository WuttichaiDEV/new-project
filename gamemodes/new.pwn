				veh_info [ vehicleid - 1 ] [ v_fuel ],
				veh_info [ vehicleid - 1 ] [ v_millage ],
				veh_info [ vehicleid - 1 ] [ v_id ] ) ;
				mysql_tquery ( mysql, query_string, "", "" ) ;

				new playerid = - 1 ;
