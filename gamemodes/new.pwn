cmd:plveh(playerid, params[])
{
    if(p_info[playerid][admin] < 4) return 1;

	extract params -> new player:tid, vehicle_id, color_1, color_2; else return SendClientMessage(playerid, col_gray, !"Gunakan: "c_white"/plveh [id pemain] [id kendaraan] [warna 1] [warna 2]");
	if(!(400 < vehicle_id < 611) || !(0 < color_1 < 255) || !(0 < color_2 < 255)) return SendClientMessage(playerid, col_gray, !"Gunakan: "c_white"/plveh [id pemain] [id kendaraan] [warna 1] [warna 2]");

	if(GetPVarInt(tid, !"admin_vehicle")) return SendClientMessage(playerid, col_white, !"Kendaraan sementara telah dibuat untuk pemain ini. Untuk menghapusnya gunakan /delveh");

	new Float:pl_pos_X, Float:pl_pos_Y, Float:pl_pos_Z;
	GetPlayerPos(tid, pl_pos_X, pl_pos_Y, pl_pos_Z);
	new adm_veh = CreateVehicle(vehicle_id, pl_pos_X, pl_pos_Y, pl_pos_Z, 0.0, color_1, color_2, -1);
	SetPVarInt(tid, !"admin_vehicle", adm_veh);

	veh_info[adm_veh - 1][v_fuel] = 100.0;

	LinkVehicleToInterior(adm_veh, GetPlayerInterior(playerid));
	SetVehicleVirtualWorld(adm_veh, GetPlayerVirtualWorld(playerid));

	PutPlayerInVehicle(tid, adm_veh, 0);
	SetPlayerArmedWeapon(tid, 0);

	new text_string[128];
	format(text_string, sizeof(text_string), ""c_grey" [A] %s[%d] membuat transportasi untuk %s[%d]. ID: %d | Model: %d", p_info[playerid][name], playerid, p_info[tid][name], tid, adm_veh, vehicle_id);
	foreach(new i: admin_players) SendClientMessage(i, col_white, text_string);

	if(playerid != tid) SendClientMessage(tid, col_green, !"Administrator memberi Anda sebuah mobil");
	return true;
}

