cmd:createfvehicle( playerid, params [ ] )
{
	if ( p_info [ playerid ] [ admin ] < 8 )return 1 ;
	if ( GetPlayerState ( playerid ) != PLAYER_STATE_DRIVER ) return SendClientMessage ( playerid, col_gray,""c_orange_red"* "c_grey"Anda harus dalam transportasi." ) ;
	if ( sscanf ( params, "dds[12]", params [ 0 ], params [ 1 ], params [ 2 ] ) )return SendClientMessage ( playerid, col_gray,""c_orange_red"* "c_grey"Gunakan: /createfvehicle [model] [organisasi] [nomor]" ) ;
