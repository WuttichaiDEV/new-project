cmd:fammute(playerid, params[])
{
    if(p_info[playerid][family] == 0) return SendClientMessage(playerid, col_white, !""scm_error"Anda bukan anggota keluarga.");
    if(p_info[playerid][family_rang] < family_info[p_info[playerid][family] - 1][fam_settings][5])
    {
        new text_string[100];
        format(text_string, sizeof(text_string), ""scm_error"Perintah tersedia dari status %s (%d)", family_rank[p_info[playerid][family] - 1][family_info[p_info[playerid][family] - 1][fam_settings][5] - 1], family_info[p_info[playerid][family] - 1][fam_settings][5]);
        return SendClientMessage(playerid, col_white, text_string);
    }
    extract params -> new player:targetid, fam_mutes, string:fam_reason[64]; else return SendClientMessage(playerid, col_gray, !"Gunakan: /fammute [ID] [Menit] [Alasan]");
    if(!IsPlayerConnected(targetid) || targetid == playerid) return SendClientMessage(playerid, col_white, !""scm_error"Pemain tidak ditemukan.");
    if(p_info[targetid][family] != p_info[playerid][family]) return SendClientMessage(playerid, col_white, !""scm_error"Pemain itu tidak ada di keluarga Anda.");
    if(p_info[targetid][family_rang]  >= p_info[playerid][family_rang]) return SendClientMessage(playerid, col_white, !""scm_error"Anda tidak dapat memblokir walkie-talkie ke manajemen Anda.");
    if(fam_mutes < 1 || fam_mutes > 120) return SendClientMessage(playerid, col_white, !""scm_info"Pemblokiran radio tidak boleh kurang dari 1 menit atau lebih dari 120 menit.");

    new family_id = p_info[playerid][family];

    new text_string[125];
    format(text_string, sizeof(text_string),"{%s}[Keluarga]"c_silver" %s %s walkie-talkie %s diblokir selama %d menit. Sebab: %s", family_info[family_id - 1][fam_chat_color], family_rank[family_id - 1][p_info[playerid][family_rang] - 1], p_info[playerid][name], p_info[targetid][name], fam_mutes, fam_reason);
    family_message(family_id, col_white, text_string);

    format(text_string, sizeof(text_string), "%s walkie-talkie %s diblokir selama %d menit. Sebab: %s", p_info[playerid][name], p_info[targetid][name], fam_mutes, fam_reason);
    write_family(playerid, family_id, TYPE_LOG_WARN, text_string);

    p_info[targetid][fam_mute] = fam_mutes * 60;
    return 1;
}

