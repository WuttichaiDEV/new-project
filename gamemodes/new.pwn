	if ( sscanf ( params, "u", params [ 0 ] ) )return SendClientMessage(playerid, col_gray,""c_orange_red"* "c_grey"Gunakan: /dmgstat [ID]" ) ;
	if ( ! IsPlayerConnected ( params [ 0 ] )  ) return SendClientMessage ( playerid, col_gray,""c_orange_red"* "c_grey"Pemain tidak ditemukan." ) ;

	global_string [ 0 ] = EOS ;
	format ( global_string, 140, ""c_white"player_gm: %d\ntick differ: %d\nkilled: %d",
