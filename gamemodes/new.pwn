stock is_text_invalid ( text [ ] )
{
	if ( strfind ( text, "'", true ) != -1 ) return 1 ;
	if ( strfind ( text, "=", true ) != -1 ) return 1 ;
	if ( strfind ( text, "%", true ) != -1 ) return 1 ;
	if ( strfind ( text, "&", true ) != -1 ) return 1 ;
	if ( strfind ( text, "*", true ) != -1 ) return 1 ;
	if ( strfind ( text, "( ", true ) != -1 ) return 1 ;
	if ( strfind ( text, ")", true ) != -1 ) return 1 ;
	if ( strfind ( text, ",", true ) != -1 ) return 1 ;
	if ( strfind ( text, "`", true ) != -1 ) return 1 ;
	if ( strfind ( text, ";", true ) != -1 ) return 1 ;
	if ( strfind ( text, "|", true ) != -1 ) return 1 ;
	return 0 ;
}

