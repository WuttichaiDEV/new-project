	}
	#include "modules/objects/m_rem_objects.pwn"
	//==============================================================================
	for ( new i = 0; i < 20; i ++ ) SendClientMessage ( playerid, -1, " " ) ;
	p_info[playerid][pTaxiCash] =
   	p_info[playerid][pTaxiTime] =
